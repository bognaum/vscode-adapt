{
  "name": "adapt",
	"displayName": "adapt",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:adapt.selectPathSegment",
        "onCommand:adapt.selectBetweenQuotes",
        "onCommand:adapt.tripleLineSplit",
        "onCommand:adapt.delToLineBegin",

		"onCommand:adapt.alignCursors",
		"onCommand:adapt.pasteCursorNums",
		"onCommand:adapt.pasteCursorNums-x",
		"onCommand:adapt.pasteCursorNums-xx",
		"onCommand:adapt.pasteCursorNums-xxx",
		"onCommand:adapt.pasteCursorNums-xxxx",
		"onCommand:adapt.pasteCursorNums-xxxxx",
		"onCommand:adapt.changeToEntities",
		"onCommand:adapt.pasteLineNumber",
		"onCommand:adapt.openSublimeMerge",
		"onCommand:adapt.openSublimeText",
		"onCommand:adapt.openCMD",
		"onCommand:adapt.openPowerShell",
		"onCommand:adapt.openProjectInSublimeMerge",
		"onCommand:adapt.openProjectInSublimeText",
		"onCommand:adapt.openProjectInCMD",
		"onCommand:adapt.openProjectInPowerShell"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"mac": "alt+cmd+shift+m",
				"win": "alt+ctrl+shift+m",
				"key": "alt+ctrl+shift+m",
				"command": "adapt.selectPathSegment",
				"when": "editorTextFocus"
			},
			{
				"mac": "alt+shift+m",
				"win": "alt+shift+m",
				"key": "alt+shift+m",
				"command": "adapt.selectBetweenQuotes",
				"when": "editorTextFocus"
			},
			{
				"mac": "shift+enter",
				"win": "shift+enter",
				"key": "shift+enter",
				"command": "adapt.tripleLineSplit",
				"when": "editorTextFocus"
			},
			{
				"mac": "cmd+shift+backspace",
				"win": "ctrl+shift+backspace",
				"key": "ctrl+shift+backspace",
				"command": "adapt.delToLineBegin",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "adapt.selectPathSegment",
				"title": "Adapt: Select path segment"
			},
			{
				"command": "adapt.selectBetweenQuotes",
				"title": "Adapt: Select between quotes"
			},
			{
				"command": "adapt.tripleLineSplit",
				"title": "Adapt: Triple line split"
			},
			{
				"command": "adapt.delToLineBegin",
				"title": "Adapt: Delite to line begin"
			},


			{
				"command": "adapt.alignCursors",
				"title": "Adapt: Align cursors"
			},
			{
				"command": "adapt.pasteCursorNums",
				"title": "Adapt: Paset Numbers of Cursors"
			},
			{
				"command": "adapt.pasteCursorNums-x",
				"title": "Adapt: Paset Numbers of Cursors X"
			},
			{
				"command": "adapt.pasteCursorNums-xx",
				"title": "Adapt: Paset Numbers of Cursors XX"
			},
			{
				"command": "adapt.pasteCursorNums-xxx",
				"title": "Adapt: Paset Numbers of Cursors XXX"
			},
			{
				"command": "adapt.pasteCursorNums-xxxx",
				"title": "Adapt: Paset Numbers of Cursors XXXX"
			},
			{
				"command": "adapt.pasteCursorNums-xxxxx",
				"title": "Adapt: Paset Numbers of Cursors XXXXX"
			},
			{
				"command": "adapt.changeToEntities",
				"title": "Adapt: Change <>& to &...;"
			},
			{
				"command": "adapt.pasteLineNumber",
				"title": "Adapt: Paste Line Number"
			},
			{
				"command": "adapt.openSublimeMerge",
				"title": "Adapt: Open Sublime Merge"
			},
			{
				"command": "adapt.openSublimeText",
				"title": "Adapt: Open Sublime Text"
			},
			{
				"command": "adapt.openCMD",
				"title": "Adapt: Open CMD"
			},
			{
				"command": "adapt.openPowerShell",
				"title": "Adapt: Open PowerShell"
			},


			{
				"command": "adapt.openProjectInSublimeMerge",
				"title": "Adapt: Open Project With Sublime Merge"
			},
			{
				"command": "adapt.openProjectInSublimeText",
				"title": "Adapt: Open Project With Sublime Text"
			},
			{
				"command": "adapt.openProjectInCMD",
				"title": "Adapt: Open Project With CMD"
			},
			{
				"command": "adapt.openProjectInPowerShell",
				"title": "Adapt: Open Project With PowerShell"
			}
		],
		"menus": {
			"editor/title/context": [
				{
					"command": "adapt.alignCursors"
				},
				{
					"command": "adapt.changeToEntities"
				},
				{
					"submenu": "pasteCursorNumbers"
				},
				{
					"command": "adapt.pasteLineNumber"
				}
			],
			"editor/title": [
				{
					"command": "adapt.alignCursors"
				},
				{
					"command": "adapt.changeToEntities"
				},
				{
					"submenu": "pasteCursorNumbers"
				},
				{
					"command": "adapt.pasteLineNumber"
				},
				{
					"command": "adapt.openSublimeMerge",
					"group": "1"
				},
				{
					"command": "adapt.openSublimeText",
					"group": "1"
				},
				{
					"command": "adapt.openCMD",
					"group": "1"
				},
				{
					"command": "adapt.openPowerShell",
					"group": "1"
				},
				{
					"submenu": "openProjectWith",
					"group": "1"
				}
			],
			"explorer/context": [
				{
					"command": "adapt.openSublimeMerge"
				},
				{
					"command": "adapt.openSublimeText"
				},
				{
					"command": "adapt.openCMD"
				},
				{
					"command": "adapt.openPowerShell"
				}
			],
			"pasteCursorNumbers": [
				{
					"command": "adapt.pasteCursorNums"
				},
				{
					"command": "adapt.pasteCursorNums-x"
				},
				{
					"command": "adapt.pasteCursorNums-xx"
				},
				{
					"command": "adapt.pasteCursorNums-xxx"
				},
				{
					"command": "adapt.pasteCursorNums-xxxx"
				},
				{
					"command": "adapt.pasteCursorNums-xxxxx"
				}
			],
			"openProjectWith": [
				{
					"command": "adapt.openProjectInSublimeMerge"
				},
				{
					"command": "adapt.openProjectInSublimeText"
				},
				{
					"command": "adapt.openProjectInCMD"
				},
				{
					"command": "adapt.openProjectInPowerShell"
				}
			]
		},
		"submenus": [
			{
				"id": "pasteCursorNumbers",
				"label": "Paste Cursor Numbers"
			},
			{
				"id": "openProjectWith",
				"label": "Adapt: Open Project Folder With ..."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
