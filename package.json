{
  "name": "adapt",
	"displayName": "adapt",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:adapt.selectPathSegment",
        "onCommand:adapt.selectBetweenQuotes",
        "onCommand:adapt.tripleLineSplit",
        "onCommand:adapt.delToLineBegin",

		"onCommand:adapt.alignCursors",
		"onCommand:adapt.pasteCursorNums",
		"onCommand:adapt.pasteCursorNums-x",
		"onCommand:adapt.pasteCursorNums-xx",
		"onCommand:adapt.pasteCursorNums-xxx",
		"onCommand:adapt.pasteCursorNums-xxxx",
		"onCommand:adapt.pasteCursorNums-xxxxx",
		"onCommand:adapt.changeToEntities",
		"onCommand:adapt.pasteLineNumber",
		"onCommand:adapt.openSublimeMerge",
		"onCommand:adapt.openSublimeText",
		"onCommand:adapt.openCMD",
		"onCommand:adapt.openPowerShell",
		"onCommand:adapt.openProjectInSublimeMerge",
		"onCommand:adapt.openProjectInSublimeText",
		"onCommand:adapt.openProjectInCMD",
		"onCommand:adapt.openProjectInPowerShell",

		"onCommand:adapt.replaceSlashes.backToFortvard",
		"onCommand:adapt.replaceSlashes.doubleBackToFortvard",
		"onCommand:adapt.replaceSlashes.forvardToBack",
		"onCommand:adapt.replaceSlashes.forvardToDoubleBack",
		"onCommand:adapt.replaceSlashes.backToDoubleBack",
		"onCommand:adapt.replaceSlashes.doubleBackToBack",

		"onCommand:adapt.copyPathName",
		"onCommand:adapt.copyPath",
		"onCommand:adapt.copyFileName",
		"onCommand:adapt.copyProjectPath",

		"onCommand:adapt.notation.dashToSnail",
		"onCommand:adapt.notation.snailToDash",
		"onCommand:adapt.notation.dashToCamel",
		"onCommand:adapt.notation.snailToCamel",
		"onCommand:adapt.notation.camelToDash",
		"onCommand:adapt.notation.camelToSnail",

		"onCommand:adapt.uriCoding.decodeURI",
		"onCommand:adapt.uriCoding.decodeURIComponent",
		"onCommand:adapt.uriCoding.encodeURI",
		"onCommand:adapt.uriCoding.encodeURIComponent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"mac": "alt+cmd+shift+m",
				"win": "alt+ctrl+shift+m",
				"key": "alt+ctrl+shift+m",
				"command": "adapt.selectPathSegment",
				"when": "editorTextFocus"
			},
			{
				"mac": "alt+shift+m",
				"win": "alt+shift+m",
				"key": "alt+shift+m",
				"command": "adapt.selectBetweenQuotes",
				"when": "editorTextFocus"
			},
			{
				"mac": "shift+enter",
				"win": "shift+enter",
				"key": "shift+enter",
				"command": "adapt.tripleLineSplit",
				"when": "editorTextFocus"
			},
			{
				"mac": "cmd+shift+backspace",
				"win": "ctrl+shift+backspace",
				"key": "ctrl+shift+backspace",
				"command": "adapt.delToLineBegin",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "adapt.selectPathSegment",
				"title": "Adapt: Select path segment"
			},
			{
				"command": "adapt.selectBetweenQuotes",
				"title": "Adapt: Select between quotes"
			},
			{
				"command": "adapt.tripleLineSplit",
				"title": "Adapt: Triple line split"
			},
			{
				"command": "adapt.delToLineBegin",
				"title": "Adapt: Delete to line begin"
			},


			{
				"command": "adapt.alignCursors",
				"title": "Adapt: Align cursors"
			},
			{
				"command": "adapt.pasteCursorNums",
				"title": "Adapt: Paste Numbers of Cursors"
			},
			{
				"command": "adapt.pasteCursorNums-x",
				"title": "Adapt: Paste Numbers of Cursors X"
			},
			{
				"command": "adapt.pasteCursorNums-xx",
				"title": "Adapt: Paste Numbers of Cursors XX"
			},
			{
				"command": "adapt.pasteCursorNums-xxx",
				"title": "Adapt: Paste Numbers of Cursors XXX"
			},
			{
				"command": "adapt.pasteCursorNums-xxxx",
				"title": "Adapt: Paste Numbers of Cursors XXXX"
			},
			{
				"command": "adapt.pasteCursorNums-xxxxx",
				"title": "Adapt: Paste Numbers of Cursors XXXXX"
			},
			{
				"command": "adapt.changeToEntities",
				"title": "Adapt: Change <>& to &...;"
			},
			{
				"command": "adapt.pasteLineNumber",
				"title": "Adapt: Paste Line Number"
			},
			{
				"command": "adapt.openSublimeMerge",
				"title": "Adapt: Open Sublime Merge"
			},
			{
				"command": "adapt.openSublimeText",
				"title": "Adapt: Open Sublime Text"
			},
			{
				"command": "adapt.openCMD",
				"title": "Adapt: Open CMD"
			},
			{
				"command": "adapt.openPowerShell",
				"title": "Adapt: Open PowerShell"
			},


			{
				"command": "adapt.openProjectInSublimeMerge",
				"title": "Adapt: Open Project With Sublime Merge"
			},
			{
				"command": "adapt.openProjectInSublimeText",
				"title": "Adapt: Open Project With Sublime Text"
			},
			{
				"command": "adapt.openProjectInCMD",
				"title": "Adapt: Open Project With CMD"
			},
			{
				"command": "adapt.openProjectInPowerShell",
				"title": "Adapt: Open Project With PowerShell"
			},



			{
				"command": "adapt.replaceSlashes.backToFortvard",
				"title": "\\ >>> /"
			},
			{
				"command": "adapt.replaceSlashes.doubleBackToFortvard",
				"title": "\\\\ >>> /"
			},
			{
				"command": "adapt.replaceSlashes.forvardToBack",
				"title": "/ >>> \\"
			},
			{
				"command": "adapt.replaceSlashes.forvardToDoubleBack",
				"title": "/ >>> \\\\"
			},
			{
				"command": "adapt.replaceSlashes.backToDoubleBack",
				"title": "\\ >>> \\\\"
			},
			{
				"command": "adapt.replaceSlashes.doubleBackToBack",
				"title": "\\\\ >>> \\"
			},
			{
				"command": "adapt.copyPathName",
				"title": "Adapt: Copy Path / Name"
			},
			{
				"command": "adapt.copyPath",
				"title": "Adapt: Copy Path"
			},
			{
				"command": "adapt.copyFileName",
				"title": "Adapt: Copy Name"
			},
			{
				"command": "adapt.copyProjectPath",
				"title": "Adapt: Copy Project Path"
			},

			{
				"command": "adapt.notation.dashToSnail",
				"title": "a-b >>> a_b"
			},
			{
				"command": "adapt.notation.snailToDash",
				"title": "a_b >>> a-b"
			},
			{
				"command": "adapt.notation.dashToCamel",
				"title": "a-b >>> aB"
			},
			{
				"command": "adapt.notation.snailToCamel",
				"title": "a_b >>> aB"
			},
			{
				"command": "adapt.notation.camelToDash",
				"title": "aB >>> a-b"
			},
			{
				"command": "adapt.notation.camelToSnail",
				"title": "aB >>> a_b"
			},


			{
				"command": "adapt.uriCoding.decodeURI",
				"title": "decodeURI( sel )"
			},
			{
				"command": "adapt.uriCoding.decodeURIComponent",
				"title": "decodeURIComponent( sel )"
			},
			{
				"command": "adapt.uriCoding.encodeURI",
				"title": "encodeURI( sel )"
			},
			{
				"command": "adapt.uriCoding.encodeURIComponent",
				"title": "encodeURIComponent( sel )"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "adapt.alignCursors"
				},
				{
					"command": "adapt.changeToEntities"
				},
				{
					"submenu": "pasteCursorNumbers"
				},
				{
					"command": "adapt.pasteLineNumber"
				},
				{
					"command": "adapt.openSublimeMerge",
					"group": "1"
				},
				{
					"command": "adapt.openSublimeText",
					"group": "1"
				},
				{
					"command": "adapt.openCMD",
					"group": "1"
				},
				{
					"command": "adapt.openPowerShell",
					"group": "1"
				},
				{
					"submenu": "openProjectWith",
					"group": "1"
				},
				{
					"submenu": "path-name"
				},
				{
					"submenu": "notations"
				},
				{
					"submenu": "uri-coding"
				}
			],
			"explorer/context": [
				{
					"command": "adapt.openSublimeMerge"
				},
				{
					"command": "adapt.openSublimeText"
				},
				{
					"command": "adapt.openCMD"
				},
				{
					"command": "adapt.openPowerShell"
				},
				{
					"submenu": "path-name-to-explorer"
				}
			],
			"pasteCursorNumbers": [
				{
					"command": "adapt.pasteCursorNums"
				},
				{
					"command": "adapt.pasteCursorNums-x"
				},
				{
					"command": "adapt.pasteCursorNums-xx"
				},
				{
					"command": "adapt.pasteCursorNums-xxx"
				},
				{
					"command": "adapt.pasteCursorNums-xxxx"
				},
				{
					"command": "adapt.pasteCursorNums-xxxxx"
				}
			],
			"openProjectWith": [
				{
					"command": "adapt.openProjectInSublimeMerge"
				},
				{
					"command": "adapt.openProjectInSublimeText"
				},
				{
					"command": "adapt.openProjectInCMD"
				},
				{
					"command": "adapt.openProjectInPowerShell"
				}
			],
			"path-name": [
				{"command": "adapt.replaceSlashes.backToFortvard"},
				{"command": "adapt.replaceSlashes.doubleBackToFortvard"},
				{"command": "adapt.replaceSlashes.forvardToBack"},
				{"command": "adapt.replaceSlashes.forvardToDoubleBack"},
				{"command": "adapt.replaceSlashes.backToDoubleBack"},
				{"command": "adapt.replaceSlashes.doubleBackToBack"},

				{"command": "adapt.copyPathName"},
				{"command": "adapt.copyPath"},
				{"command": "adapt.copyFileName"},
				{"command": "adapt.copyProjectPath"}
			],
			"path-name-to-explorer": [
				{"command": "adapt.copyPathName"},
				{"command": "adapt.copyPath"},
				{"command": "adapt.copyFileName"},
				{"command": "adapt.copyProjectPath"}
			],
			"notations": [
				{"command": "adapt.notation.dashToSnail"},
				{"command": "adapt.notation.snailToDash"},
				{"command": "adapt.notation.dashToCamel"},
				{"command": "adapt.notation.snailToCamel"},
				{"command": "adapt.notation.camelToDash"},
				{"command": "adapt.notation.camelToSnail"}
			],
			"uri-coding": [
				{"command": "adapt.uriCoding.decodeURI"},
				{"command": "adapt.uriCoding.decodeURIComponent"},
				{"command": "adapt.uriCoding.encodeURI"},
				{"command": "adapt.uriCoding.encodeURIComponent"}
			]
		},
		"submenus": [
			{
				"id": "pasteCursorNumbers",
				"label": "Adapt: Paste Cursor Numbers ..."
			},
			{
				"id": "openProjectWith",
				"label": "Adapt: Open Project Folder With ..."
			},
			{
				"id": "path-name",
				"label": "Adapt: Path / Name: ..."
			},
			{
				"id": "path-name-to-explorer",
				"label": "Adapt: Path / Name: ..."
			},
			{
				"id": "notations",
				"label": "Adapt: aB a-b a_b ..."
			},
			{
				"id": "uri-coding",
				"label": "Adapt: URI Coding ..."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
